{
  "name": "boilermaker",
  "version": "1.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "git checkout -b deploy && webpack && git add -f public/bundle.js public/bundle.js.map && git commit --allow-empty -m 'Deploying' && git push --force heroku deploy:master && git checkout master && git branch -D deploy",
    "start": "node server",
    "seed": "node server/seed.js",
    "start-dev": "npm run build-client-watch & npm run start-server",
    "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/getaway-test' mocha ./**/*.spec.js --compilers js:babel-register"
  },
